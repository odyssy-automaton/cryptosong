{
  "version": 2,
  "name": "cryptosong",
  "builds": [
    {
      "src": "server/server-config.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server-config.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "WEB_HOST": "@web_host",
      "MONGODB_HOST": "@mongodb_host",
      "REACT_APP_API_HOST": "@react_app_api_host"
    }
  }
}
